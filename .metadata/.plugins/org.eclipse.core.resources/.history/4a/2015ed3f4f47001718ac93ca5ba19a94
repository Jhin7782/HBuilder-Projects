<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<title></title>
	</head>
	<body>
		<script type="text/javascript">
			var arr = [];
			function factorial (num) {
				if (num >= 10) {
					return Math.random();
				} else {
					arr.push(factorial(num + 1))
					return arr;
				}
			}
			
			console.log(factorial(0));
		</script>
	</body>
</html>
