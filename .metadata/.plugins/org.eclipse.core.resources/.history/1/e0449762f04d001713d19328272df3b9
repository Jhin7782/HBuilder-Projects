		<ul id="myList">
			<li>li1</li>
			<li>li2</li>
			<li>li3</li>
		</ul>
		
		<script type="text/javascript">
			var myList = document.getElementById("myList");
			var CN = myList.childNodes;
			var CN_len = myList.childNodes.length;
			for (var i = 0; i < CN.length; i++) {
				console.log(CN[i].nodeName);
			}
			console.log(CN_len);
		</script>
