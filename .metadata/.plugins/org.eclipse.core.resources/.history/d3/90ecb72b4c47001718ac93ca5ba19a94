	<body>
		<script type="text/javascript">
			function factorial (num) {
				if (num >= 10) {
					console.log(Math.random());
					return;
				} else {
					console.log(factorial(num + 1));
					return;
				}
			}
			
			factorial(1);
		</script>
	</body>
</html>
